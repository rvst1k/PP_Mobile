<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Clocky1">
    <LinearLayout
        android:layout_marginTop="50dp"
        android:id="@+id/main3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/white"
        >
        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Расписание"
            android:textStyle="italic"
            android:textSize="36sp"
            android:textColor="@color/black"
            android:layout_gravity="center"
            android:textAlignment="center"
            />
        <LinearLayout
            android:id="@+id/make11"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="@drawable/layout_bg">
           <LinearLayout
               android:layout_width="180dp"
               android:layout_height="100dp"
               android:orientation="vertical">
               <TextView
                   android:id="@+id/time11"
                   android:layout_width="70dp"
                   android:layout_height="wrap_content"
                   android:text="6:30"
                   android:textStyle="bold"
                   android:textSize="30sp"
                   android:layout_marginLeft="15dp"
                   android:layout_marginTop="5dp">
               </TextView>
               <TextView
                   android:id="@+id/timename11"
                   android:layout_width="180dp"
                   android:layout_height="wrap_content"
                   android:text="Покормить"
                   android:textSize="30sp"
                   android:layout_marginLeft="15dp"
                   android:layout_marginTop="10dp">
               </TextView>
           </LinearLayout>
              <Switch
                  android:id="@+id/switch11"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:scaleX="2"
                  android:scaleY="2"
                  android:layout_marginLeft="80dp"
                  android:layout_marginTop="40dp"
                  >
              </Switch>
             <ImageView
                 android:id="@+id/deletebutton11"
                 android:layout_width="43dp"
                 android:layout_height="43dp"
                 android:layout_marginTop="30dp"
                 android:layout_marginLeft="20dp"
                 android:src="@drawable/deleteiconh">

             </ImageView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/make22"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="@drawable/layout_bg">
            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="100dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/time22"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="7:00"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="5dp">
                </TextView>
                <TextView
                    android:id="@+id/timename22"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:text="Погулять"
                    android:textSize="30sp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp">
                </TextView>
            </LinearLayout>
            <Switch
                android:id="@+id/switch22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleX="2"
                android:scaleY="2"
                android:layout_marginLeft="80dp"
                android:layout_marginTop="40dp"
                >
            </Switch>
            <ImageView
                android:id="@+id/deletebutton22"
                android:layout_width="43dp"
                android:layout_height="43dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/deleteiconh">

            </ImageView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/make33"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="@drawable/layout_bg">
            <LinearLayout
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/time33"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="8:30"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="5dp">
                </TextView>
                <TextView
                    android:id="@+id/timename33"
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:text="Помыть"
                    android:textSize="30sp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp">
                </TextView>
            </LinearLayout>
            <Switch
                android:id="@+id/switch33"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleX="2"
                android:scaleY="2"
                android:layout_marginLeft="110dp"
                android:layout_marginTop="40dp"
                >
            </Switch>
            <ImageView
                android:id="@+id/deletebutton33"
                android:layout_width="43dp"
                android:layout_height="43dp"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/deleteiconh">

            </ImageView>
        </LinearLayout>
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/plus"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="320dp">
        </ImageView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="730dp">
        <ImageView
            android:id="@+id/profile"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/user"
            android:layout_gravity="center"
            />

        <ImageView
            android:id="@+id/friends"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/friends"
            android:layout_gravity="center"
            />

        <ImageView
            android:id="@+id/loved"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/heart"
            android:layout_gravity="center"
            />
        <ImageView
            android:id="@+id/time1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/clock"
            android:layout_gravity="center"
            />
        <ImageView
            android:id="@+id/geo"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/geo"
            android:layout_gravity="center"
            />
    </LinearLayout>
</RelativeLayout>